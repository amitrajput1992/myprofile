#!/usr/bin/env bash
declare -A git_command

git_command=(["s"]="status" ["a"]="add" ["b"]="branch" ["c"]="commit" ["cm"]="commit" ["co"]="checkout" ["d"]="diff" ["amend"]="commit --amend" ["force"]="push --force" ["r"]="remote")

cmd1=$1
if [ ${#git_command[$cmd1]} -gt 0 ]
then
  shift
  git ${git_command[$cmd1]} $@
else
  git $@
fi
